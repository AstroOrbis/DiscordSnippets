{
  "name": "discordsnippets",
  "displayName": "Discord Snippets",
  "description": "A simple tool that adds snippets for Discord.JS.",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:messageembed.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./js_snippets/Embed.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./js_snippets/Client.code-snippets"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "discord.js": "^13.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstroOrbis/DiscordSnippets.git"
  },
  "keywords": [
    "Discord",
    "Snippets",
    "Discord.JS"
  ],
  "author": "Astro Orbis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AstroOrbis/DiscordSnippets/issues"
  },
  "homepage": "https://github.com/AstroOrbis/DiscordSnippets#readme",
  "publisher": "AstroOrbis"
}
